{
  "parser": "@typescript-eslint/parser",
	"parserOptions": {
		"sourceType": "module"
	},
  "plugins": [
    "@typescript-eslint",
		"unused-imports"
  ],
  "extends": [
		"eslint:recommended",
		"plugin:@typescript-eslint/recommended",
		"prettier",
  ],
  "root": true,
	"rules": {
		"unused-imports/no-unused-imports": "error"
	}
}
